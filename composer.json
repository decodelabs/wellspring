{
    "name": "decodelabs/wellspring",
    "description": "PHP autoload management tools",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
